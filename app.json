{
  "expo": {
    "name": "Sai Baba Miracles",
    "slug": "sai-baba-miracles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "com.saibabamiracles.app",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbfcfa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saibabamiracles.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbfcfa"
      },
      "package": "com.saibabamiracles.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-web-browser",
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.646552958318-aukbcpnkl451p7vfcfc8g4p85e5f2l3j"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b5842246-36e1-4c88-94c7-8a3bb5b0d22c"
      }
    },
    "owner": "kameeshwaran89"
  }
}
